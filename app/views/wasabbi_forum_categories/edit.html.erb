<h1>Editing wasabbi_forum_category</h1>

<% form_for(@wasabbi_forum_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @wasabbi_forum_category %> |
<%= link_to 'Back', wasabbi_forum_categories_path %>
