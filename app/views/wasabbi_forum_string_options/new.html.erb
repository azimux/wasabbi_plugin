<h1>New wasabbi_forum_string_option</h1>

<% form_for(@wasabbi_forum_string_option) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :wasabbi_forum_id %><br />
    <%= f.text_field :wasabbi_forum_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', wasabbi_forum_string_options_path %>
