<h1>Listing wasabbi_modships</h1>

<table>
  <tr>
    <th>Wasabbi user</th>
    <th>Is super mod</th>
    <th>Forum</th>
    <th>Is subforum mod</th>
  </tr>

<% for wasabbi_modship in @wasabbi_modships %>
  <tr>
    <td><%=h wasabbi_modship.wasabbi_user_id %></td>
    <td><%=h wasabbi_modship.is_super_mod %></td>
    <td><%=h wasabbi_modship.forum_id %></td>
    <td><%=h wasabbi_modship.is_subforum_mod %></td>
    <td><%= link_to 'Show', wasabbi_modship %></td>
    <td><%= link_to 'Edit', edit_wasabbi_modship_path(wasabbi_modship) %></td>
    <td><%= link_to 'Destroy', wasabbi_modship, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

