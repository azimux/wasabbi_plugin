<h1>New wasabbi_adminship</h1>

<% form_for(@wasabbi_adminship) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :wasabbi_user_id %><br />
    <%= f.text_field :wasabbi_user_id %>
  </p>
  <p>
    <%= f.label :is_super_admin %><br />
    <%= f.check_box :is_super_admin %>
  </p>
  <p>
    <%= f.label :forum_id %><br />
    <%= f.text_field :forum_id %>
  </p>
  <p>
    <%= f.label :is_subforum_admin %><br />
    <%= f.check_box :is_subforum_admin %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', wasabbi_adminships_path %>
