<h1>Editing wasabbi_forum</h1>

<% form_for(@wasabbi_forum) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @wasabbi_forum %> |
<%= link_to 'Back', wasabbi_forums_path %>
