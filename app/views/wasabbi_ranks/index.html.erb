<h1>Listing wasabbi_ranks</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Lbound</th>
    <th>Rbound</th>
  </tr>

<% for wasabbi_rank in @wasabbi_ranks %>
  <tr>
    <td><%=h wasabbi_rank.name %></td>
    <td><%=h wasabbi_rank.lbound %></td>
    <td><%=h wasabbi_rank.rbound %></td>
    <td><%= link_to 'Show', wasabbi_rank %></td>
    <td><%= link_to 'Edit', edit_wasabbi_rank_path(wasabbi_rank) %></td>
    <td><%= link_to 'Destroy', wasabbi_rank, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New wasabbi_rank', new_wasabbi_rank_path %>
